<div class="content" id="about" <%- data.page === 'about' ? '' : 'hidden' %>>
  <div class="content-boundaries">
    <div id="propic-container">
      <div id="propic">
        <picture>
          <source srcset="images/me-vintage.webp" type="image/webp">
          <source srcset="images/me-vintage.jpg" type="image/jpeg">
          <img id="me-vintage" src="images/me-vintage.jpg" alt="Me" title="Me" width="101" height="125">
        </picture>
        <img id="polaroid-frame" src="images/polaroid-frame.svg" alt="" width="114" height="138">
      </div>
      <img id="pin" src="images/pin.svg" alt="" width="24" height="26">
    </div>

    <% data.pages.about.bio.forEach(str => { %>
      <p><%- str.replace(/%age%/g, `<span id="age">${data.age}</span>`) %></p>
    <% }) %>
  </div>
</div>
